alias get_global_ip="curl ipinfo.io/ip | pbcopy | echo "\n" | pbpaste"
alias get_local_ip="ifconfig | grep -A 1 'wlp2s0' | grep 'inet' | awk '{print \$2}' | pbcopy | pbpaste"
alias ls="ls -a --color=auto"
alias done="notify-send -i face-wink 'done'"
alias dc="docker-compose"
#alias fzf="/home/jaskaran/.fzf/bin/fzf"
alias cat="bat"
alias nvm='nocorrect nvm'
alias tree='nocorrect tree'
alias n=nvim
